<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#00435F" />
            <gradient android:angle="-90" android:startColor="#0086BE" android:endColor="#4DC2F2"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#00435F" />
            <solid android:color="#0d7199"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#00658E" />
            <gradient android:angle="-90" android:startColor="#4DC2F2" android:endColor="#0086BE" />
        </shape>
    </item>
</selector>